{
  "name": "convrtrjs",
  "version": "1.1.0",
  "description": "A simple JavaScript based tool to quickly convert text between different formats",
  "main": "index.html",
  "scripts": {
    "dev": "sass --watch --update --style=expanded scss:assets/devcss",
    "prod": "sass --no-source-map --style=compressed scss:_site/assets/css",
    "build": "npx @11ty/eleventy --formats=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stickerboy/convrtrjs.git"
  },
  "bugs": {
    "url": "https://github.com/stickerboy/convrtrjs/issues"
  },
  "author": "Kenny Cameron",
  "license": "ISC",
  "homepage": "https://www.kenny.cx/convrtrjs",
  "dependencies": {
    "bootstrap-scss": "^5.3.2",
    "@11ty/eleventy": "^2.0.1"
  }
}

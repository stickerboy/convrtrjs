<!doctype html>
<html lang="en" {% if page-id=="fourohfour" %} class="h-100" {% endif %}>

    <head>
        {% include "layout/meta" %}
        {% include "layout/title" %}

        {% render "layout/css" %}
        {% include "layout/favicons" %}
    </head>

    <body id="IOOBI9O4IEIEOIO4O4IB" {% if page-id=="fourohfour" %}class="d-flex flex-column h-100 top-0" {% endif %}>
        <a class="visually-hidden-focusable" href="#content">Skip to main content</a>

        {% include "layout/header" %}

        <div class="grid sidebar-grid gap-0 p-0">
            <div class="nav-bar p-0 position-sticky overflow-y-auto overflow-x-hidden">
                {% include "layout/sidebar" %}
            </div>
            <main class="page-wrap p-0 mw{% if page-id == "fourohfour" %} flex-shrink-0 g-start-2 d-flex flex-column{% endif %}"
                 data-bs-smooth-scroll="true" data-bs-threshold="0,1" data-bs-root-margin="10% 0% -90%" id="content">
                {% if page-id != "fourohfour" -%}
                {% include "partials/default" %}
                {%- endif -%}
                {{ content }}
                {% include "layout/footer" %}
            </main>
        </div>

        {% include "layout/scripts" %}
        {% render "layout/toast" %}
    </body>

</html>
{% assign dynamicPartialPath = "partials/routes/" | append: page-id | append: ".liquid" %}
{% if dynamicPartialPath | fileExists %}
{% render dynamicPartialPath %}
{% else %}
{% render "partials/routes/home" %}
{% endif %}
<script src="{{ '/assets/js/core/bootstrap.bundle.min.js' | url }}" crossorigin="anonymous"></script>
{%- if page-id == "sitemap" %}
<script src="https://cdn.jsdelivr.net/npm/masonry-layout@4.2.2/dist/masonry.pkgd.min.js"
    integrity="sha384-GNFwBvfVxBkLMJpYMOABq3c+d3KnQxudP/mGPkzpZSTYykLBNsZEnG2D9G/X/+7D" crossorigin="anonymous"
    async></script>
{%- endif %}
<script type="module" src="{{ '/assets/js/theme.mjs' | url }}"></script>
{%- if page-id != "404" %}
<script type="module" src="{{ '/assets/js/clip.mjs' | url }}"></script>
{%- endif %}
{% if page-id == "ciphers" or page-id == "hash" %}
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/4.0.0/crypto-js.min.js"></script>
{%- endif -%}
<script type="module" src="{{ '/assets/js/scripts.mjs' | url }}"></script>
{%- if page-id == "check" or page-id == "files" -%}
<script>const HEADERS = {{ headers | json }};</script>
{%- endif -%}
{%- if page-id == "elementstable" or page-id == "misc" -%}
<script>const ELEMENTS = {{ elements | json }};</script>
{%- endif -%}
{%- if page-id == "exif" or page-id == "files" -%}
<script src="{{ '/assets/js/files/exif-reader.js' | url }}"></script>
{%- endif %}
<script src="{{ '/pagefind/pagefind-ui.js' | url }}"></script>
{%- if page-id == 'search' -%}
<script type="module" src="{{ '/assets/js/search.mjs' | url }}"></script>
{%- else %}
<script type="module" src="{{ '/assets/js/search-global.mjs' | url }}"></script>
{%- endif %}
{%- if page-id == 'settings' -%}
<script type="module" src="{{ '/assets/js/settings/settings.mjs' | url }}"></script>
{%- endif %}
